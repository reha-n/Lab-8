#include <iostream>
using namespace std;

int main() {
    int length, width, area;

    cout << "Enter the length of the fence in feet: ";
    cin >> length;

    cout << "Enter the width of the fence in feet: ";
    cin >> width;

    area = length * width;

    cout << "The area of the fence is: " << area << endl;

    return 0;
}
